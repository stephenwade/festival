{
  "name": "festival-admin",
  "description": "Festival admin",
  "license": "ISC",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenwade/festival.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node index.js",
    "start:dev": "nodemon --exec \"npm run start\"",
    "lint": "cd .. && npm run lint",
    "format": "cd .. && npm run format",
    "test": "echo \"No tests yet\" && exit 1",
    "clean": "rm -rf ui/dist",
    "prebuild": "npm run clean",
    "build": "cd ui && rollup -c rollup.config.js",
    "deploy": "dotenv -e ../.env -- ./deploy.sh"
  },
  "dependencies": {
    "@koa/router": "10.1.0",
    "@magic-sdk/admin": "1.3.0",
    "@open-wc/building-rollup": "1.10.0",
    "@rollup/plugin-replace": "2.4.2",
    "deepmerge": "4.2.2",
    "dotenv": "10.0.0",
    "koa": "2.13.1",
    "koa-mount": "4.0.0",
    "koa-send": "5.0.1",
    "koa-static": "5.0.0",
    "lit": "2.0.0-rc.2",
    "rollup": "2.54.0"
  },
  "devDependencies": {
    "dotenv-cli": "4.0.0",
    "nodemon": "2.0.12"
  }
}
